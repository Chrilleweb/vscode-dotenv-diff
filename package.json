{
  "name": "dotenv-diff",
  "displayName": "dotenv-diff",
  "description": "Warns about missing or unused .env variables",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Linters"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "workspaceContains:**/.env"
  ],
  "contributes": {},
  "sideEffects": false,
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "release": "release-it",
    "release:dry": "release-it --dry-run"
  },
  "author": "Chrilleweb",
  "publisher": "Chrilleweb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chrilleweb/vscode-dotenv-diff.git"
  },
  "bugs": {
    "url": "https://github.com/Chrilleweb/vscode-dotenv-diff/issues"
  },
  "icon": "images/logo.png",
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
